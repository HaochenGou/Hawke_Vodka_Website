{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "build",
    "source": "/service-worker.js", 
    "headers": [{"key": "Cache-Control", "value": "no-cache"}],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
